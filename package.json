{
  "name": "serverless-express-starter",
  "version": "1.1.0",
  "description": "A tool you can simply use one command to create the project for the Serverless and Express. Support both Flow and TypeScript.",
  "main": "dist/index.js",
  "bin": {
    "serverless-express-starter": "./bin/index.js"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src -d dist",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "chalk": "^2.4.2",
    "del": "^4.0.0",
    "eslint": "^5.15.2",
    "execa": "^1.0.0",
    "flow-bin": "^0.95.1",
    "inquirer": "^6.2.2",
    "yaml": "^1.4.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-flow-strip-types": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "jest": "^24.5.0",
    "rimraf": "^2.6.3"
  },
  "keywords": [
    "serverless",
    "express",
    "starter",
    "template",
    "boilerplate",
    "serverless-express",
    "flow",
    "eslint"
